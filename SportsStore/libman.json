{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery@3.3.1",
      "files": [
        "jquery.min.js",
        "jquery.js",
        "jquery.min.map"
      ],
      "destination": "wwwroot/lib/jquery/"
    },
    {
      "provider": "unpkg",
      "library": "bootstrap@4.1.3",
      "destination": "wwwroot/lib/bootstrap/"
    },

    {
      "provider": "unpkg",
      "library": "font-awesome-5-css@5.4.1",
      "destination": "wwwroot/lib/fontawesome",
      "files": [
        "css/all.css",
        "css/all.min.css",
        "css/brands.css",
        "css/brands.min.css",
        "css/fa-solid.css",
        "css/fa-solid.min.css"
      ]
    }

  ]
}